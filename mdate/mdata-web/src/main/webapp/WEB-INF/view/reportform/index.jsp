yId(noid);            var n = noid.substr(noid.length - 1);            if (noid.length == 4) {                var ul = document.getElementById(noid.substring(0, 3)).getElementsByTagName("ul");                var h2 = document.getElementById(noid.substring(0, 3)).getElementsByTagName("h2");                for (var i = 0; i < h2.length; i++) {                    h2[i].innerHTML = h2[i].innerHTML.replace("-", "+");                    h2[i].style.color = "";                }                obj.style.color = "#FF0000";                for (var i = 0; i < ul.length; i++) {                    if (i != n) {                        ul[i].className = "no";                    }                }            } else {                var span = document.getElementById("menu").getElementsByTagName("span");                var h1 = document.getElementById("menu").getElementsByTagName("h1");                for (var i = 0; i < h1.length; i++) {                    h1[i].innerHTML = h1[i].innerHTML.replace("-", "+");                    h1[i].style.color = "";                }                obj.style.color = "#08ff06";                for (var i = 0; i < span.length; i++) {                    if (i != n) {                        span[i].className = "no";                    }                }            }            if (block.className == "no") {                block.className = "";                obj.innerHTML = obj.innerHTML.replace("+", "-");            } else {                block.className = "no";                obj.style.color = "";            }        }    </script></head><body><div id="splitter-container">    <div id="MySplitter" style="height: 100%;">        <div id="LeftPane">            <div class="left-list">                <div id="menu">                    <h1 code="0102" onClick="ShowMenu(this,'NO0')" class="nav nav-pills nav-stacked">+                        采购报表分析</h1>                    <span id="NO0" class="no">   <h2 style