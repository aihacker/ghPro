<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="wxgh.entity.common.disease.DiseaseZx">

        <select id="getZX" parameterType="wxgh.param.common.disease.QueryDisease" resultType="DiseaseZx">
                SELECT * FROM t_disease_zx
                <where>
                        <if test="applyId != null">
                                `apply_id` = #{applyId}
                        </if>
                        <if test="id != null">
                                AND `id` = #{id}
                        </if>
                </where>
        </select>
</mapper>
